{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "env": {
    "SKIP_ENV_VALIDATION": "1"
  },
  "functions": {
    "api/python/[...path].py": {
      "runtime": "python@3.12",
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/agents/:path*",
      "destination": "/api/python/:path*"
    }
  ]
}